<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Access rights for backup restore wizard -->
    <record id="access_saas_backup_restore_wizard_manager" model="ir.model.access">
        <field name="name">saas.backup.restore.wizard.manager</field>
        <field name="model_id" ref="model_saas_backup_restore_wizard"/>
        <field name="group_id" ref="group_backup_manager"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
</odoo>
